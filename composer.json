{
  "name": "larra-press/blog-poster",
  "description": "Automatized poster for your Laravel based blog. Configure the scraper to properly get the data from the selected third party resource, test it, set a Job and enjoy.",
  "license": "MIT",
  "keywords": [
    "laravel",
    "larrapress",
    "scrape",
    "scraping",
    "autoposter"
  ],
  "type": "library",
  "authors": [
    {
      "name": "LarraPress BlogPoster",
      "email": "alexey.khachatryan@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "^6.0|^7.0|^8.0",
    "spatie/laravel-enum": "^2.5",
    "symfony/css-selector": "^5.3",
    "symfony/dom-crawler": "^5.3"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0|^5.0|^6.0",
    "phpunit/phpunit": "^8.4|^9.0"
  },
  "autoload": {
    "psr-4": {
      "LarraPress\\BlogPoster\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LarraPress\\BlogPoster\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "LarraPress\\BlogPoster\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
